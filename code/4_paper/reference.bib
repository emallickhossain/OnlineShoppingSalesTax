@article{asplundFribergWilander2007,
  title={Demand and distance: evidence on cross-border shopping},
  author={Asplund, Marcus and Friberg, Richard and Wilander, Fredrik},
  journal={Journal of public Economics},
  volume={91},
  number={1-2},
  pages={141--157},
  year={2007},
}
Pretty good study utilizing variation in price levels and tax changes and as a result, they trace out a wide range of price differentials across different alcoholic beverages. No major criticism here except they don't look at online shopping

@article{mikesell1970,
  title={Central cities and sales tax rate differentials: The border city problem},
